.my {
  background-color: #f3f4f5;
  padding: 20rpx;
  color: #003159;
}
.user-info {
  display: flex;
  align-items: center;
  border-radius: 20rpx;
  background-color: #fff;
}
.userinfo-avatar {
  overflow: hidden;
  width: 120rpx;
  height: 120rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.userinfo-avatar image{
  width: 100%;
  height: 100%;
}
.icon-touxiang {
  font-size: 120rpx;
}
.user-info .name {
  display: flex;
  flex-direction: column;
  font-size: 32rpx;
}
.user-info .name button{
  padding: 0;
  margin-top: 14rpx;
  font-size: 22rpx;
  background: none;
  width: auto;
  font-weight: normal;
  color: #bbb;
  text-align: left;
}
.menu {
  margin-top: 40rpx;
  width: 100%;
  background-color: #fff;
  border-radius: 20rpx;
}
.menu .item{
  position: relative;
  border-bottom: 2rpx solid #f2f2f2;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 24rpx;
}
.menu .item view {
  display: flex;
  align-items: center;
}
.menu .item:last-child{
  border-bottom: none
}
.menu .item view .iconfont {
  width: 50rpx;
  font-size: 36rpx;
  margin-right: 30rpx;
  color: #003159;
}
.contact-btn {
  position: absolute;
  right: 0;
  left: 0;
  width: 100%!important;
  height: 100%;
  z-index: 1;
  opacity: 0;
}
